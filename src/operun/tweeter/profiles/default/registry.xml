<registry>

  <records interface='Products.CMFPlone.interfaces.IResourceRegistry' prefix="plone.resources/counterup">
    <value key="js">++resource++operun.tweeter/counterup.min.js</value>
    <value key="deps">jquery</value>
  </records>

  <records interface='Products.CMFPlone.interfaces.IResourceRegistry' prefix="plone.resources/waypoints">
    <value key="js">++resource++operun.tweeter/waypoints.min.js</value>
    <value key="deps">jquery</value>
  </records>

  <records interface='Products.CMFPlone.interfaces.IResourceRegistry' prefix="plone.resources/tweeter">
    <value key="js">++resource++operun.tweeter/tweeter.js</value>
    <value key="deps">jquery</value>
    <value key="css">
      <element>++resource++operun.tweeter/tweeter.less</element>
    </value>
  </records>

  <records interface='Products.CMFPlone.interfaces.IBundleRegistry' prefix="plone.bundles/tweeter">
    <value key="resources">
      <element>tweeter</element>
    </value>
    <value key="jscompilation">++resource++operun.tweeter/tweeter-compiled.js</value>
    <value key="csscompilation">++resource++operun.tweeter/tweeter-compiled.css</value>
    <value key="enabled">True</value>
    <value key="compile">True</value>
    <value key="depends">plone</value>
    <value key="last_compilation"></value>
  </records>

</registry>
